    "def make_dashboard(x, gdp_change, unemployment, title, file_name):\n",
    "    output_file(file_name)                                                                              \n",
    "    p = figure(title=title, x_axis_label='year', y_axis_label='%')                                      \n",
    "    p.line(x.squeeze(), gdp_change.squeeze(), color=\"yellow\", line_width=4, legend=\"% gdp_change\")   \n",
    "    p.line(x.squeeze(), unemployment.squeeze(), color=\"green\", line_width=4, legend=\"%  unemployment\")\n",
    "    show(p)                   "
   ]
